---
import IFrameContainer from "@components/IFrameContainer.astro";
import { LEAGUE } from "@lib/constants";

export function getStaticPaths() {
  return [
    {
      params: {
        section: "siba",
        generated: "standings",
      },
      props: {
        file: "SIBAStandings",
        title: "SIBA League Standings",
      },
    },
    {
      params: {
        section: "siba",
        generated: "leaders",
      },
      props: {
        file: "SIBALeagueLeaders",
        title: "SIBA League Leaders",
      },
    },
    {
      params: { section: "siba", generated: "schedule" },
      props: { file: "Schedule", title: "Schedule" },
    },
    {
      params: {
        section: "siba",
        generated: "transactions",
      },
      props: {
        file: "Transactions",
        title: "Transactions",
      },
    },
    {
      params: { section: "siba", generated: "free-agents" },
      props: { file: "FA", title: "Available Free Agents" },
    },
    {
      params: {
        section: "siba",
        generated: "available-coaches",
      },
      props: {
        file: "FACoach",
        title: "Available Coaches",
      },
    },
    {
      params: {
        section: "siba",
        generated: "player-index",
      },
      props: {
        file: "PlayerIndex",
        title: "Active Player Index",
      },
    },
    {
      params: {
        section: "dbl",
        generated: "standings",
      },
      props: {
        file: "DBLStandings",
        title: "DBL League Standings",
      },
    },
    {
      params: {
        section: "dbl",
        generated: "leaders",
      },
      props: {
        file: "DBLLeagueLeaders",
        title: "DBL League Leaders",
      },
    },
    {
      params: {
        section: "news",
        generated: "headlines",
      },
      props: {
        file: "Headlines",
        title: "SIBA Headlines",
      },
    },
  ];
}

const { file, title } = Astro.props;
---

<IFrameContainer file={file} league={LEAGUE.pro} title={title} />@lib/constants
